9e352adacc08"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$Coordinates", "=", 
   RowBox[{"{", 
    RowBox[{"t", ",", "x1", ",", "x2", ",", "x3"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$PerturbationSymbol", " ", "=", " ", "\[CurlyEpsilon]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$Metric", " ", "=", " ", 
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {
       RowBox[{"1", "+", 
        RowBox[{"2", " ", "\[CurlyEpsilon]", " ", 
         RowBox[{"\[Psi]", "@", "spacetime"}]}]}], "0", "0", "0"},
      {"0", 
       RowBox[{"-", " ", 
        RowBox[{"(", 
         RowBox[{"1", " ", "+", " ", 
          RowBox[{"2", " ", "\[CurlyEpsilon]", " ", 
           RowBox[{"\[Chi]", "@", "spacetime"}]}]}], ")"}]}], "0", "0"},
      {"0", "0", 
       RowBox[{"-", " ", 
        RowBox[{"(", 
         RowBox[{"1", " ", "+", " ", 
          RowBox[{"2", " ", "\[CurlyEpsilon]", " ", 
           RowBox[{"\[Chi]", "@", "spacetime"}]}]}], ")"}]}], "0"},
      {"0", "0", "0", 
       RowBox[{"-", " ", 
        RowBox[{"(", 
         RowBox[{"1", " ", "+", " ", 
          RowBox[{"2", " ", "\[CurlyEpsilon]", " ", 
           RowBox[{"\[Chi]", "@", "spacetime"}]}]}], ")"}]}]}
     }], "\[NoBreak]", ")"}]}], ";"}]}], "Input",
